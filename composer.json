{
    "name": "yii1tech/web",
    "description": "Provides the enhanced components for the web request processing in Yii1 application",
    "keywords": ["yii1", "web", "http", "request", "cookie", "load balancer"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yii1tech/web/issues",
        "wiki": "https://github.com/yii1tech/web/wiki",
        "source": "https://github.com/yii1tech/web"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "yiisoft/yii": "~1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 || ^7.0 || ^8.0 || ^9.3 || ^10.0.7"
    },
    "autoload": {
        "psr-4": {
            "yii1tech\\web\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yii1tech\\web\\test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}